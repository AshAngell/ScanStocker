{
  "name": "app",
  "private": true,
  "scripts": {
    "build": "esbuild app/javascript/entry.tsx --bundle --outfile=app/assets/builds/application.js",
    "watch": "esbuild app/javascript/entry.tsx --bundle --outfile=app/assets/builds/application.js --watch",
